{
  "name": "osm-teams",
  "version": "2.0.0",
  "description": "Teams for OpenStreetMap!",
  "homepage": "https://mapping.team",
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/osm-teams.git"
  },
  "bugs": {
    "url": "https://github.com/developmentseed/osm-teams/issues"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "MIT",
  "scripts": {
    "docs:update-version": "node -p \"JSON.stringify({'apiFolder': 'src/pages/api','schemaFolders': ['models'],'definition': {'openapi': '3.0.0','info': {'title': 'OSM Teams API Docs','version': require('./package.json').version}}}, null, 2)\" > next-swagger-doc.json",
    "docs:generate": "yarn next-swagger-doc-cli next-swagger-doc.json",
    "docs:validate": "yarn docs:update-version && yarn docs:generate && swagger-cli validate public/swagger.json",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e:dev": "NODE_ENV=test start-server-and-test dev http://127.0.0.1:3000/api cy:open",
    "e2e": "NODE_ENV=test start-server-and-test dev http://127.0.0.1:3000/api cy:run",
    "dev": "next dev",
    "migrate": "knex migrate:latest",
    "migrate:test": "NODE_ENV=test knex migrate:latest",
    "test": "NODE_ENV=test start-server-and-test dev http://127.0.0.1:3000/api test:ava",
    "test:ava": "NODE_ENV=test ava",
    "lint": "next lint",
    "build": "next build",
    "start": "NODE_ENV=production node app/index.js",
    "postinstall": "yarn run next telemetry disable > /dev/null"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "body-parser": "^1.19.1",
    "chance": "^1.1.8",
    "compression": "^1.7.3",
    "connect-session-knex": "^2.1.1",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^2.28.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.2",
    "express-boom": "^2.0.0",
    "express-pino-logger": "^4.0.0",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.17.2",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.0",
    "knex": "^2.3.0",
    "knex-postgis": "^0.14.3",
    "leaflet": "^1.7.1",
    "leaflet-control-geocoder": "^1.13.0",
    "next": "^13.0.1",
    "next-auth": "^4.16.4",
    "next-connect": "^0.13.0",
    "next-swagger-doc": "^0.3.6",
    "node-fetch": "^2.6.7",
    "passport-light": "^1.0.1",
    "passport-oauth": "^1.0.0",
    "passport-openstreetmap": "^0.1.2",
    "pg": "^8.7.1",
    "pino": "^5.17.0",
    "pino-pretty": "^2.5.0",
    "qs": "^6.10.3",
    "querystring": "^0.2.1",
    "ramda": "^0.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^2.8.0",
    "react-modal": "^3.14.4",
    "react-select": "^5.2.2",
    "react-toastify": "^8.2.0",
    "reactjs-popup": "^1.5.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "session-file-store": "^1.5.0",
    "simple-oauth2": "^2.5.2",
    "sinon": "^7.5.0",
    "sqlite3": "^5.1.2",
    "supertest": "^4.0.2",
    "swagger-ui-react": "^4.15.5",
    "url-join": "^4.0.0",
    "url-parse": "^1.5.9",
    "url-regex": "^5.0.0",
    "xml2js": "^0.4.23",
    "yamljs": "^0.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@panva/hkdf": "^1.0.1",
    "ava": "^5.1.0",
    "babelify": "^10.0.0",
    "cypress": "^11.1.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jose": "^4.11.0",
    "nodemon": "^1.19.4",
    "nyc": "^15.1.0",
    "polished": "^4.2.2",
    "prettier": "2.7.1",
    "start-server-and-test": "^1.14.0"
  }
}
